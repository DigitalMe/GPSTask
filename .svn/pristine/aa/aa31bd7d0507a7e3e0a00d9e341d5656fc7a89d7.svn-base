package com.kajun258456357159.gps.task.vo;

public class TaskLocationVO {

	private long _categoryId;
	private long _locationNameId;
	private long _locationId;
	private CoordinateVO _coordinates;
	
	public TaskLocationVO(long categoryId, long locationId, CoordinateVO coordinatesVO){
		set_categoryId(categoryId);
		set_locationId(locationId);
		set_coordinates(coordinatesVO);
	}
	
	public TaskLocationVO(){
		CoordinateVO coordinates = new CoordinateVO();
		set_coordinates(coordinates);
	}


	public long get_categoryId() {
		return _categoryId;
	}

	public void set_categoryId(long categoryId) {
		this._categoryId = categoryId;
	}

	public CoordinateVO get_coordinates() {
		return _coordinates;
	}

	public void set_coordinates(CoordinateVO coordinates) {
		this._coordinates = coordinates;
	}

	public long get_locationId() {
		return _locationId;
	}

	public void set_locationId(long locationId) {
		this._locationId = locationId;
	}

	public long get_locationNameId() {
		return _locationNameId;
	}

	public void set_locationNameId(long _locationNameId) {
		this._locationNameId = _locationNameId;
	}
}
